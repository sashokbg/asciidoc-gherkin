{
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "strict": true,
    "baseUrl": "./",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "noImplicitAny": false
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
}
